      <visual name="vis"><geometry><box><size>10 0.2 1.0</size></box></geometry></visual>
    </link>
    <!-- east -->
    <link name="wall_e">
      <pose>5 0 0.5 0 0 1.57079632679</pose>
      <collision name="col"><geometry><box><size>10 0.2 1.0</size></box></geometry></collision>
      <visual name="vis"><geometry><box><size>10 0.2 1.0</size></box></geometry></visual>
    </link>
    <!-- west -->
    <link name="wall_w">
      <pose>-5 0 0.5 0 0 1.57079632679</pose>
