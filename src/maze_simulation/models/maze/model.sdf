      <visual name="vis"><geometry><box><size>10 0.2 1.0</size></box></geometry></visual>
    </link>
    <!-- inner divider -->
    <link name="wall_inner">
      <pose>0 0 0.5 0 0 1.57079632679</pose>
      <collision name="col"><geometry><box><size>6 0.2 1.0</size></box></geometry></collision>
      <visual name="vis"><geometry><box><size>6 0.2 1.0</size></box></geometry></visual>
    </link>
